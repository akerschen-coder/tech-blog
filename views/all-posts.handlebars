<div>
    {{#each posts}}
    <div>
        <div>
            <h2><a href="/post/{{id}}">{{title}}</a></h2>
            <div>
                posted by {{User.user_name}} on {{format_date createdAt}}
            </div>
        </div>
        <div>
            {{body}}
        </div>
    </div>
    {{/each}}
</div>